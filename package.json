{
  "name": "fake-data-generator",
  "version": "0.1.5",
  "description": "Just a small script to create fake data files given a JSON model",
  "author": "sgobotta <santiago@camba.coop>",
  "main": "dist/main.js",
  "bin": "dist/cli.js",
  "scripts": {
    "start": "node dist/cli.js",
    "build": "node_modules/.bin/rimraf dist/ && node_modules/.bin/babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./npm-debug.log --copy-files",
    "changelog": "github_changelog_generator && git add CHANGELOG.md && git commit -am \"Updates changelog\"",
    "generate": "npm run build && npm start",
    "prepublish": "npm run build",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "faker": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "rimraf": "^2.6.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "parser": "babel-eslint",
      "sourceType": "module"
    }
  },
  "homepage": "https://github.com/Cambalab/fake-data-generator#readme",
  "keywords": [
    "faker",
    "fake-data",
    "node",
    "open-source",
    "script"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cambalab/fake-data-generator.git"
  }
}
